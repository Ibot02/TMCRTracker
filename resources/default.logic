# Default logic for the randomizer - Doesn't allow glitches, but allows unintuitive tricks

!name - Default
!version - 0.5.5
!crc - E8637292

!flag - Setting - KEYSANITY - Keysanity - false
!flag - Setting - SHUFFLE_ELEMENTS -  Shuffle elements into item pool
!flag - Setting - GLITCHLESS - Disable glitches - true
!flag - Setting - WHYWOULDYOUDOTHISTOYOURSELF - Obscure spots
!flag - Setting - RUPEEMANIA - Add available rupees to pool
!flag - Setting - WEAPONS - Damaging items other than sword seen as weapons
!flag - Setting - TRAPS - Shuffle traps in itempool
!flag - Setting - TIMEDOHKO - Die in one hit if the timer runs out
!flag - Setting - MUSIC_RANDO - Randomize Music
!dropdown - Setting - FUSION_SETTING - No normal fusions - NO_FUSIONS - Open fusion mode - OPENFUSIONS - Vanilla Fusions - VANILLA_FUSIONS
!dropdown - Setting - FUSION_SKIPS - Allow fusion skips - ALLOW_FUSION_SKIPS - Instant fusion skip - INSTANT_SKIP - No fusion skips - NO_FUSION_SKIP
!dropdown - Setting - DHC_SETTING - Ped Open/Regular DHC - NORMALDHC - Ped Finish/No DHC - NODHC - Open DHC - OPENDHC - Open DHC + Ped Items - PEDITEMS

# these 2 are only valid outside of openDHC modes, if both are set to 0 it defaults to 4 elements
!dropdown - Setting - SWORD_SETTING - No Sword Ped - 0SWORD - Smith Sword Ped - 1 - White Sword Ped - 2 - Red Sword Ped - 3 - Blue Sword Ped - 4 - Four Sword Ped - 5
!dropdown - Setting - ELEMENT_SETTING - 4 Element Ped - 4 - 3 Element Ped - 3 - 2 Element Ped - 2 - 1 Element Ped - 1 - No Element Ped - 0ELEMENT

!flag - Gimmick - RANDOM_LANGUAGE - Randomize language
!flag - Gimmick - RAINBOW_HEARTS - Enable Rainbow Hearts
!color - Gimmick - TUNIC_COLOR - Change Tunic Color -    10 - FF - 08    -    10 - 7B - 29    -    41 - A0 - 28    -    BD - FF - A0
!color - Gimmick - HEART_COLOR - Change Heart Color -    FF - 50 - 10
!color - Gimmick - SPLIT_COLOR - Change Split Bar Color -    4A - FF - 18     -    31 - 9C - 18    -   31 - 52 - 18
!dropdown - Gimmick - FOLLOWER_SETTING - No Follower - NOFOLLOWER - Mailman Follower - 0x04 - Zelda Follower - 0x05 - Malon Follower - 0x1E - Smith Follower - 0x22 - King Gustaf Follower - 0x24 - Cow Follower - 0x31 - Goron Follower - 0x32 - Anju Follower - 0x45

!eventdefine - customRNG - 0x`RAND_INT`

#Gimmicks

!define - `FOLLOWER_SETTING`
!ifndef - NOFOLLOWER
    !eventdefine - follower
    !eventdefine - followerID - `FOLLOWER_SETTING`
!endif
!undefine - `FOLLOWER_SETTING` 

!dropdown - Gimmick - FUZZY_SETTING - No Fuzziness - NOFUZZ - Fuzz 0 - 0x0 - Fuzz 1 - 0x1 - Fuzz 2 - 0x2 - Fuzz 3 - 0x3 - Fuzz 4 - 0x4 - Fuzz 5 - 0x5 - Fuzz 6 - 0x6 - Fuzz 7 - 0x7 - Fuzz 8 - 0x8 - Fuzz 9 - 0x9 - Fuzz 10 - 0xA - Fuzz 11 - 0xB - Fuzz 12 - 0xC - Fuzz 13 - 0xD - Fuzz 14 - 0xE

!define - `FUZZY_SETTING`
!ifndef - NOFUZZ
    !eventdefine - fuzzy - `FUZZY_SETTING`
!endif
!undefine - `FUZZY_SETTING` 

!ifdef - TUNIC_COLOR
    !eventdefine - TUNIC_COLORED
    !eventdefine - TUNIC_COLOR_DARKEST - 0x`TUNIC_COLOR_1`
    !eventdefine - TUNIC_COLOR_DARK - 0x`TUNIC_COLOR_2`
    !eventdefine - TUNIC_COLOR_MAIN - 0x`TUNIC_COLOR_0`
    !eventdefine - TUNIC_COLOR_LIGHT - 0x`TUNIC_COLOR_3`
!endif

!ifdef - RAINBOW_HEARTS
    !eventdefine - heartscolor
    !eventdefine - heartscolorrainbow
!else
    !ifdef - HEART_COLOR
        !eventdefine - heartscolor
        !eventdefine - heartFillR - (0x`HEART_COLOR_0` & 0x1F)
        !eventdefine - heartFillG - (0x`HEART_COLOR_0` >> 5)   & 0x1F
        !eventdefine - heartFillB - (0x`HEART_COLOR_0` >> 10) & 0x1F
    !endif
!endif

!ifdef - SPLIT_COLOR
    !eventdefine - barcolor
    !eventdefine - splitFirst - 0x`SPLIT_COLOR_0`
    !eventdefine - splitThird - 0x`SPLIT_COLOR_1`
	!eventdefine - splitFourth - 0x`SPLIT_COLOR_2`
!endif


#Settings

# Add fusion setting and skip defines so it works like before
!define - `FUSION_SETTING`
!define - `FUSION_SKIPS`

!define - `DHC_SETTING`

!ifdef - NO_FUSION_SKIP
    !eventdefine - NOFUSE
!endif

!define - `SWORD_SETTING`
!ifdef - 0SWORD
    !define - SWORDEX
	!define - SWORDLEVEL - 0
!else
    !define - SWORDLEVEL - `SWORD_SETTING`
!endif
!undefine - `SWORD_SETTING`

!define - `ELEMENT_SETTING`
!ifdef - 0ELEMENT
    !define - ELEMENTEX
	!define - ELEMENTAMOUNT - 0
!else
    !define - ELEMENTAMOUNT - `ELEMENT_SETTING`
!endif
!undefine - `ELEMENT_SETTING`

#make sure that only 1 of these settings is active at the same time
!ifdef - PEDITEMS
    !eventdefine - openDHC
    !eventdefine - pedestalItems
!else
    !ifdef - OPENDHC
        !eventdefine - openDHC
    !else
	    !ifdef - NODHC
		    !eventdefine - noDHC
        !endif

        !ifndef - SWORDEX
		    !eventdefine - requirementSwordTier       - `SWORDLEVEL`
			GotSwords; Helper; ; (+`SWORDLEVEL`, Items.SmithSword)
        !endif

        !ifndef - ELEMENTEX
		    !eventdefine - requirementElementsAmmount - `ELEMENTAMOUNT` 
			GotElements; Helper; ; (+`ELEMENTAMOUNT`, Items.WaterElement, Items.FireElement, Items.WindElement, Items.EarthElement)
        !endif

        !ifndef - SWORDEX
            !ifndef - ELEMENTEX
                #both
                CompletePed; Helper; ; Helpers.GotSwords, Helpers.GotElements
		    !else
                #sword
                CompletePed; Helper; ; Helpers.GotSwords
		    !endif
	    !else
	        !ifndef - ELEMENTEX
		        #element
			    CompletePed; Helper; ; Helpers.GotElements
		    !else
		        #neither
			    CompletePed; Helper; ; (+4, Items.WaterElement, Items.FireElement, Items.WindElement, Items.EarthElement)
		    !endif
	    !endif
	!endif
!endif

!ifdef - SHUFFLE_ELEMENTS
    !eventdefine - globalElements - 1   # if defined, does not draw elements on map (during full shuffle)
!endif

